<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="claire.example.com.androidannotations">

    <uses-permission android:name="android.permission.INTERNET"/>

    <!-- Etape 2 : remplacer "UILApplication" par "UILApplication_" -->
    <application android:name=".application.UILApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- Etape 2 : remplacer "MainActivity" par "MainActivity_" -->
        <activity android:name="claire.example.com.androidannotations.activities.MainActivity"

            android:label="@string/titre_rebus">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Etape 2 : remplacer "GameActivity" par "GameActivity_" -->
        <activity android:name="claire.example.com.androidannotations.activities.GameActivity"

            android:label="@string/titre_rebus" />
    </application>

</manifest>